@{
    var redirectLogin = "~/login.cshtml";
    var pageTitle = "Statistics";
    var db = Database.Open("102547-concepttest");

    
}

<!DOCTYPE html>

<html lang="en">
   <head>
        <meta charset="utf-8" />
        <link rel="stylesheet" type="text/css" href="StyleSheet.css">
        <title>@pageTitle</title>
    </head>
    <body>
        <div>
            <h1>Statistics</h1>
            <hr>
            @{
